{
  "ci": {
    "collect": {
      "staticDistDir": "./"
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "content-width": ["warn"],
        "apple-touch-icon": ["warn"],
        "csp-xss": ["warn"],
        "font-display": ["warn"],
        "frame-title": ["warn"],
        "installable-manifest": ["warn"],
        "maskable-icon": ["warn"],
        "meta-description": ["warn"],
        "service-worker": ["warn"],
        "splash-screen": ["warn"],
        "themed-omnibox": ["warn"],
        "unused-css-rules": ["warn"],
        "unused-javascript": ["warn"],
        "uses-rel-preconnect": ["warn"],
        "irst-meaningful-paint": ["warn"],
        "interactive": ["warn"],
        "largest-contentful-paint": ["warn"],
        "legacy-javascript": ["warn"],
        "max-potential-fid": ["warn"],
        "render-blocking-resources": ["warn"],
        "uses-long-cache-ttl": ["warn"]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "lhci"
    }
  }
}
